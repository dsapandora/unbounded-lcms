<div class="media">
  <div class="media-left">
    <a href="#">
      <img class="media-object" alt="64x64" src="http://placehold.it/64x64" style="width: 64px; height: 64px;">
    </a>
  </div>
  <div class="media-body">
    <p>
        <% if lobject.sources.engageny && lobject.sources.engageny.any? { |source| !source.active } %>
        <strong>Not active</strong>
        <% end %>
        <%= link_to(lobject.title, unbounded_show_path(id: lobject.id)) %>
    </p>
    <div class="well list-description">
        <%= lobject.description.try(:html_safe) %>
    </div>
  </div>
</div>
